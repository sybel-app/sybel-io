{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
  },
  "compileOnSave": true,
  "include": [
    "src"
  ], 
  "exclude": ["**/src/generated-types/**/*"]
}
